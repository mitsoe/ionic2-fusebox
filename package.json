  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/jasmine": "^2.5.43",
    "@types/lodash": "^4.14.44",
    "@types/node": "^7.0.5",
    "fuse-box": "^1.3.38",
    "jasmine": "^2.5.3",
    "node-sass": "^4.1.1",
